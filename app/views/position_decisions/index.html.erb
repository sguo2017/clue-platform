<% case @user.position
    when "decision" %>
        <%= render 'position_decision', position_decision: @position_decision  %>
 <% when "action" %>
        <%= render 'position_action', position_decision: @position_decision  %>
 <% when "analysis" %>
      <%= render 'position_analysis', position_decision: @position_decision  %>
 <% else
    end %>
